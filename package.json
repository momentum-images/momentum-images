{
  "name": "momentum-images",
  "homepage": "https://momentumimages.co.uk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "bulma": "^0.9.1",
    "fastest-validator": "^1.8.0",
    "gh-pages": "^3.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-lazy-images": "^1.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "sitemap": "node scripts/create-sitemap.js",
    "generate-images": "node scripts/generate-images.js",
    "prebuild": "npm run generate-images",
    "postbuild": "react-snap && npm run sitemap",
    "gh-pages": "gh-pages -d build",
    "deploy": "npm run build && npm run gh-pages",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true,
      "browser": true
    },
    "rules": {
      "react/prop-types": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-standard": "^1.0.2"
  }
}
